<div id="<%= dom_id restaurant %>">
  <p>
    <strong>Name:</strong>
    <%= restaurant.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= restaurant.address %>
  </p>

<strong> Reviews for <%= restaurant.name%> </strong>
<ul>
<% restaurant.reviews.each do |review| %>
  <li><%= review.content%> </li>
<% end %>
</ul>


</div>
